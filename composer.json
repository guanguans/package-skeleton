    "config": {
        "allow-plugins": {
            "bamarni/composer-bin-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "hyperf/composer-plugin": true,
            "infection/extension-installer": true,
            "kylekatarnls/update-helper": true,
