    "prefer-stable": true,
    "autoload": {
        "psr-0": {},
        "psr-4": {
            "Guanguans\\PackageSkeleton\\": "src"
        },
        "classmap": [],
        "files": [],
        "exclude-from-classmap": [
            "benchmarks"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Guanguans\\PackageSkeletonTests\\": "tests"
        }
    },
    "bin": [],
    "archive": {},
    "config": {
        "allow-plugins": {
            "infection/extension-installer": true,
            "kylekatarnls/update-helper": true,
            "mrsuh/php-generics": true,
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "hyperf/composer-plugin": true,
            "bamarni/composer-bin-plugin": true,
            "wikimedia/composer-merge-plugin": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        },
        "apcu-autoloader": true,
        "classmap-authoritative": false,
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-main": "2.x-dev"
        },
        "hooks": {
            "post-merge": [
                "composer checks"
            ],
            "pre-commit": [
                "composer checks"
            ]
        },
        "phpstan": {
            "includes": []
        },
        "update-helper": "Guanguans\\PackageSkeleton\\UpdateHelper"
    },
    "scripts": {
        "post-install-cmd": [
            "@cghooks add --ignore-lock",
            "@cghooks update"
        ],
        "post-update-cmd": [
            "@cghooks update"
        ],
        "post-autoload-dump": [
            "UpdateHelper\\UpdateHelper::check",
            "@php ./vendor/bin/testbench package:discover --ansi"
        ],
        "auto-commit": "auto-commit",
        "auto-commit-dry-run": "@auto-commit --dry-run",
        "benchmark": "@php ./vendor/bin/phpbench run --retry-threshold=3 --iterations=15 --revs=1000 --warmup=2 --ansi",
        "cghooks": "@php ./vendor/bin/cghooks --ansi",
        "changelog": "@php ./vendor/bin/conventional-changelog --ansi",
        "changelog-keep": "@php ./vendor/bin/keep-a-changelog",
        "changelog-major": "@changelog --major",
        "changelog-minor": "@changelog --minor",
        "changelog-patch": "@changelog --patch",
        "checks": [
            "@mark-start",
            "@md-lint",
            "@mark-separate",
            "@style-lint",
            "@mark-separate",
            "@test",
            "@mark-finish"
        ],
        "composer-check-platform-reqs": "@composer check-platform-reqs --lock --ansi",
        "composer-require-checker": "@php ./vendor/bin/composer-require-checker check --config-file=composer-require-checker.json composer.json --ansi",
        "composer-unused-checker": "@php ./vendor/bin/composer-unused --ansi",
        "composer-validate": "@composer validate --check-lock --strict --ansi",
        "doctum": "@php ./vendor/bin/doctum.php update doctum.php --ansi",
        "infection": "@php ./vendor/bin/infection --test-framework=pest --show-mutations --threads=4 --ansi",
        "lint": [
            "for DIR in .; do find $DIR -maxdepth 1 -type f -name '*.php' -type f ! -name 'xxx.php' -exec php -l {} \\; 2>&1 | (! grep -v '^No syntax errors detected'); done",
            "for DIR in ./benchmarks ./laravel/src ./src ./tests ./yii2/src; do find $DIR -type f -name '*.php' -type f ! -name 'xxx.php' -exec php -l {} \\; 2>&1 | (! grep -v '^No syntax errors detected'); done"
        ],
        "mark-finish": "printf '\\n!\\n!\\t\\033[0;32m%s\\033[0m\\n!\\n\\n' \"Finished\"",
        "mark-separate": "printf '\\n!\\n!\\t\\033[0;33m%s\\033[0m\\n!\\n\\n' \"----------------\"",
        "mark-start": "printf '\\n!\\n!\\t\\033[0;36m%s\\033[0m\\n!\\n\\n' \"Started\"",
        "md-fix": "@md-lint --fix",
        "md-lint": "lint-md --config .lintmdrc ./*.md ./.github/ ./docs/",
        "pest": "@php ./vendor/bin/pest --cache-result-file=./build//phpunit/.phpunit.result.cache",
        "pest-coverage": "@pest --coverage-html=./build/phpunit/ --coverage-clover=clover.xml --coverage",
        "phparkitect": "@php ./vendor/bin/phparkitect check --ansi",
        "phpmnd": "@php ./vendor/bin/phpmnd ./src/ --exclude=./vendor/ --non-zero-exit-on-violation --ansi",
        "phpstan": "@php ./vendor/bin/phpstan analyse --xdebug --ansi -vv",
        "phpstan-baseline": "@phpstan --generate-baseline --allow-empty-baseline",
        "pint": "@php ./vendor/bin/pint --ansi -v",
        "pint-test": "@pint --test",
        "post-merge": [
            "@composer install"
        ],
        "psalm": "@php ./vendor/bin/psalm",
        "psalm-baseline": "@psalm --update-baseline",
        "rector": "@php ./vendor/bin/rector --clear-cache --ansi -v",
        "rector-dry-run": "@rector --dry-run",
        "release": "@php ./vendor/bin/monorepo-builder release --ansi -vvv",
        "style-fix": "@php ./vendor/bin/php-cs-fixer fix --using-cache=no --config=.php-cs-fixer.php --ansi",
        "style-lint": "@style-fix --diff --dry-run",
        "test": "@pest",
        "test-coverage": "@pest-coverage",
        "test-phpunit": "@php ./vendor/bin/phpunit --cache-result-file=./build/phpunit/.phpunit.result.cache --coverage-text --ansi",
        "test-phpunit-coverage": "@test --coverage-html=./build/phpunit/ --coverage-clover=clover.xml",
        "test-watch": [
            "Composer\\Config::disableProcessTimeout",
            "./vendor/bin/phpunit-watcher watch --ansi < /dev/tty"
        ]
    },
    "scripts-descriptions": {}
}
