        "tomasvotruba/bladestan": "*",
        "tomasvotruba/cognitive-complexity": "*",
        "tomasvotruba/phpstan-bodyscan": "*",
        "tomasvotruba/type-coverage": "*",
        "tomasvotruba/unused-public": "*",
